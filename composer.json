{
    "name": "masoudi/nova-progress-field",
    "description": "Laravel Nova progress bar field",
    "keywords": [
        "laravel",
        "nova",
        "progressbar",
        "masoudi"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Masoudi\\Nova\\Fields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Masoudi\\Nova\\Fields\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
